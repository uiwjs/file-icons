{
  "name": "@uiw/file-icons",
  "version": "1.3.0",
  "description": "File icons in the file tree",
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/file-icons"
  },
  "homepage": "https://uiwjs.github.io/file-icons",
  "scripts": {
    "font": "svgtofont --sources icon --output fonts",
    "copy": "copyfiles -f fonts/react/*.d.ts lib --verbose && copyfiles -f fonts/react/*.d.ts esm --verbose",
    "build": "tsbb build --entry fonts/react/index.js --no-source-maps",
    "start": "npm run font && npm run build && npm run copy"
  },
  "keywords": [
    "file-icons",
    "svg",
    "icon",
    "font",
    "webfont",
    "icon",
    "iconfont",
    "font-face",
    "compress",
    "minify",
    "font-cli",
    "otf",
    "ttf",
    "woff",
    "eot",
    "svg",
    "ttf2eot",
    "ttf2woff",
    "ttf2svg",
    "svg2ttf",
    "css",
    "base64"
  ],
  "author": "Kenny <wowohoo@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "copyfiles": "~2.4.1",
    "svgtofont": "~3.17.5",
    "tsbb": "~3.7.2"
  }
}
